{
  "kind": "collectionType",
  "collectionName": "ai_tools",
  "info": {
    "singularName": "ai-tool",
    "pluralName": "ai-tools",
    "displayName": "AITool"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "thumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "ai_tools"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.rich-text",
        "shared.media",
        "shared.quote"
      ]
    }
  }
}
