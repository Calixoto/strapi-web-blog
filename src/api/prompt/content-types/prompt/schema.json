{
  "kind": "collectionType",
  "collectionName": "prompts",
  "info": {
    "singularName": "prompt",
    "pluralName": "prompts",
    "displayName": "Prompt",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true,
      "maxLength": 256
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "prompts"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.media",
        "shared.quote",
        "shared.rich-text"
      ]
    }
  }
}
